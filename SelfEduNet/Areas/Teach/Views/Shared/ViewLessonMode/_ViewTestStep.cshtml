@model Step

@if (Model?.StepTest?.GoogleFormUrl is { Length: > 0 })
{
	<div class="test-block">
		<iframe src="@Model?.StepTest?.GoogleFormUrl"
		        width="750" height="1300" frameborder="0" marginheight="0" marginwidth="0">
			Завантаження…
		</iframe>
	</div>
}
<div class="d-flex justify-content-center">
	<button type="button" name="check-test-results" class="btn btn-success" data-step-id="@Model.Id">Показати результати</button>
</div>
